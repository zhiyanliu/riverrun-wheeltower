{
  "coreThing": {
    "caPath": "root-ca.crt",
    "certPath": "rr-video-stream-demo-greengrass-core-thing-cert.crt",
    "keyPath": "rr-video-stream-demo-greengrass-core-thing-private.key",
    "thingArn": "<CORE_THING_ARN>",
    "iotHost": "<IOT_SERVICE_ENDPOINT>",
    "ggHost": "<GREENGRASS_SERVICE_ENDPOINT>",
    "keepAlive": 600
  },
  "runtime": {
    "cgroup": {
      "useSystemd": "yes"
    },
    "allowFunctionsToRunAsRoot": "yes"
  },
  "managedRespawn": false,
  "crypto": {
    "caPath": "file://certs/root-ca.crt",
    "principals": {
      "IoTCertificate": {
        "privateKeyPath": "file://certs/rr-video-stream-demo-greengrass-core-thing-private.key",
        "certificatePath": "file://certs/rr-video-stream-demo-greengrass-core-thing-cert.crt"
      },
      "SecretsManager": {
        "privateKeyPath": "file://certs/rr-video-stream-demo-greengrass-core-thing-private.key"
      }
    }
  }
}